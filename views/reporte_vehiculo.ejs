<form method="post" id="">
    <!--___________________________CONTENIDO______________________________-->
    <div class="content-wrapper">
      <section class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1>Reporte veh√≠culos</h1>
            </div>
          </div>
        </div>
      </section>
      <!-- Main content -->
      <section class="content">
        <div id="resultados_ajax4"></div>
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-10 offset-md-1">
              <div class="card card-success card-outline p-2">
                <div class="modal-body">
                  <div class="form-row">
                    <h5>Recibidos</h5>
                    <div class="col-md-2 my-5">
                      <label>Fecha Desde</label>
                      <input type="text" name="fechadesde" id="fechadesde" class="form-control datepicker" placeholder="" required onKeyPress="" />
                    </div>
                    <div class="col-md-2 my-5">
                      <label>Fecha Hasta</label>
                      <input type="text" name="fechahasta" id="fechahasta" class="form-control datepicker" placeholder="" required onKeyPress="" />
                    </div>
                </div>
                </div>
                <div class="modal-footer">
                  <input type="button" onClick="generarpdfrecibido()" value="Generar PDF" class="btn btn-success pull-left" />
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <section class="content">
        <div id="resultados_ajax4"></div>
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-10 offset-md-1">
              <div class="card card-success card-outline p-2">
                <div class="modal-body">
                  <div class="form-row">
                    <h5>Cobrados</h5>
                    <div class="col-md-2 my-5">
                      <label>Fecha Desde</label>
                      <input type="text" name="fechadesde2" id="fechadesde2" class="form-control datepicker" placeholder="" required onKeyPress="" />
                    </div>
                    <div class="col-md-2 my-5">
                      <label>Fecha Hasta</label>
                      <input type="text" name="fechahasta2" id="fechahasta2" class="form-control datepicker" placeholder="" required onKeyPress="" />
                    </div>
                </div>
                </div>
                <div class="modal-footer">
                  <input type="button" onClick="generarpdffacturado()" value="Generar PDF" class="btn btn-success pull-left" />
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
    </div>


    </form>

    <% include footer %>


<script>

    function generarpdfrecibido(){
      var fechadesde  = $("#fechadesde").val();
      var fechahasta  = $("#fechahasta").val();

      window.open("reportes/control/vehiculosrecibidos.pdf?fechadesde=" + fechadesde +"&fechahasta=" + fechahasta);
  }
  function generarpdffacturado(){
    var fechadesde2  = $("#fechadesde2").val();
    var fechahasta2  = $("#fechahasta2").val();

    window.open("reportes/control/vehiculosfacturados.pdf?fechadesde=" + fechadesde2 +"&fechahasta=" + fechahasta2);
}


    function fnv_soloNumeros(e){
    var keynum = window.event ? window.event.keyCode : e.which;

      if ((keynum == 8) || (keynum == 46))
      return true;

      return /\d/.test(String.fromCharCode(keynum));
    }
    function soloLetrasConEspacio(e) {
      tecla = (document.all) ? e.keyCode : e.which;
      if (tecla==8) return true; // backspace
      if (tecla==32) return true; // espacio

      patron = /[a-zA-Z]/; //patron

      te = String.fromCharCode(tecla);
      return patron.test(te); // prueba de patron
    }
    $(document).ready(function() {
      $('.datepicker').datepicker({
        format: 'dd-mm-yyyy',
        autoclose: true,
        endDate: new Date()

      });
    });



</script>
